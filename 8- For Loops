##For Loops
myList = ["a","b","c","d","e"]
for x in myList:
    print(x)
    if x == "b":
     break

for x in myList:
    if x == "c":
     continue
    print(x)
    
for x in range(0,30,3):
    print(x)

##nested loops
colors = ["blue","red"]
pill = ["pill"]

for x in colors:
    for y in pill:
        print(x,y)

## pass
for x in [0,1,3,5,6,7]:
    pass